{
  "openapi": "3.0.3",
  "info": {
    "title": "Online-Radio",
    "description": "Online-Radio",
    "termsOfService": "http://ingress-dev.ma/vwasp-dev-onlineradio-service-token",
    "contact": {
      "name": "ma",
      "url": "https://www.google.com",
      "email": "https://www.google.com"
    },
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:17792",
      "description": "Inferred Url"
    }
  ],
  "tags": [
    {
      "name": "访问令牌模块接口",
      "description": "Token Controller"
    }
  ],
  "paths": {
    "/{version}/onlineradio/token-info/access-token": {
      "post": {
        "tags": [
          "访问令牌模块接口"
        ],
        "summary": "【VWASP-ONLINERADIO-3092】获取访问令牌信息",
        "operationId": "getAccessTokenUsingPOST",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BaseDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultOfAccessTokenVO"
                }
              }
            }
          }
        }
      }
    },
    "/{version}/onlineradio/token-info/access-token-list": {
      "get": {
        "tags": [
          "访问令牌模块接口"
        ],
        "summary": "【VWASP-ONLINERADIO-3094】访问令牌信息列表",
        "operationId": "getAccessTokensUsingGET",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultOfListOfAccessTokenVO"
                }
              }
            }
          }
        }
      }
    },
    "/{version}/onlineradio/token-info/add": {
      "post": {
        "tags": [
          "访问令牌模块接口"
        ],
        "summary": "【VWASP-ONLINERADIO-3090】保存访问令牌信息",
        "operationId": "addUsingPOST",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AccessTokenDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultOfboolean"
                }
              }
            }
          }
        }
      }
    },
    "/{version}/onlineradio/token-info/delete": {
      "post": {
        "tags": [
          "访问令牌模块接口"
        ],
        "summary": "【VWASP-ONLINERADIO-3091】删除访问令牌信息",
        "operationId": "deleteUsingPOST",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BaseDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultOfboolean"
                }
              }
            }
          }
        }
      }
    },
    "/{version}/onlineradio/token-info/guest-access-token": {
      "post": {
        "tags": [
          "访问令牌模块接口"
        ],
        "summary": "【VWASP-ONLINERADIO-3093】获取游客访问令牌信息",
        "operationId": "getGuestTokenUsingPOST",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BaseDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultOfstring"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AccessTokenDTO": {
        "title": "AccessTokenDTO",
        "required": [
          "accessToken",
          "expiresIn",
          "refreshToken",
          "uid"
        ],
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "description": "访问令牌"
          },
          "expiresIn": {
            "type": "integer",
            "description": "访问令牌的失效时间，单位为秒",
            "format": "int64"
          },
          "refreshToken": {
            "type": "string",
            "description": "刷新令牌，用于刷新访问令牌"
          },
          "scope": {
            "type": "string",
            "description": "获得的授权范围，使用英文逗号分隔",
            "example": "subscribe:write,subscribe:read,open_pay:read"
          },
          "uid": {
            "type": "integer",
            "description": "第三方用户ID",
            "format": "int32"
          }
        },
        "description": "访问令牌数据实体类"
      },
      "AccessTokenVO": {
        "title": "AccessTokenVO",
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "description": "访问令牌"
          },
          "bizCode": {
            "type": "string",
            "description": "业务编码"
          },
          "bizUserId": {
            "type": "string",
            "description": "业务用户编码"
          },
          "uid": {
            "type": "integer",
            "description": "第三方用户ID",
            "format": "int32"
          }
        },
        "description": "访问令牌数据实体类"
      },
      "BaseDTO": {
        "title": "BaseDTO",
        "type": "object",
        "description": "基础参数信息实体类"
      },
      "Result": {
        "title": "Result",
        "type": "object",
        "properties": {
          "data": {
            "type": "object"
          },
          "errorCode": {
            "type": "string"
          },
          "respCode": {
            "type": "integer",
            "format": "int32"
          },
          "respMsg": {
            "type": "string"
          },
          "traceId": {
            "type": "string"
          }
        }
      },
      "ResultOfAccessTokenVO": {
        "title": "ResultOfAccessTokenVO",
        "type": "object",
        "properties": {
          "data": {
            "$ref": "#/components/schemas/AccessTokenVO"
          },
          "errorCode": {
            "type": "string"
          },
          "respCode": {
            "type": "integer",
            "format": "int32"
          },
          "respMsg": {
            "type": "string"
          },
          "traceId": {
            "type": "string"
          }
        }
      },
      "ResultOfListOfAccessTokenVO": {
        "title": "ResultOfListOfAccessTokenVO",
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AccessTokenVO"
            }
          },
          "errorCode": {
            "type": "string"
          },
          "respCode": {
            "type": "integer",
            "format": "int32"
          },
          "respMsg": {
            "type": "string"
          },
          "traceId": {
            "type": "string"
          }
        }
      },
      "ResultOfboolean": {
        "title": "ResultOfboolean",
        "type": "object",
        "properties": {
          "data": {
            "type": "boolean"
          },
          "errorCode": {
            "type": "string"
          },
          "respCode": {
            "type": "integer",
            "format": "int32"
          },
          "respMsg": {
            "type": "string"
          },
          "traceId": {
            "type": "string"
          }
        }
      },
      "ResultOfstring": {
        "title": "ResultOfstring",
        "type": "object",
        "properties": {
          "data": {
            "type": "string"
          },
          "errorCode": {
            "type": "string"
          },
          "respCode": {
            "type": "integer",
            "format": "int32"
          },
          "respMsg": {
            "type": "string"
          },
          "traceId": {
            "type": "string"
          }
        }
      }
    }
  }
}